@model IEnumerable<WebExam.Models.ExamHisModel>

@{
    ViewBag.Title = "Index";
    var i = 1;
}


<div class="content-heading row">
    <div class="viewcat">
        <label type="button" class="fa fa-angle-double-left" style="font-weight:bold; font-size:30px;" onclick="history.back();"> - </label><label><h2 class=" lable">Danh sách bài đã làm</h2></label>
        <div style="font-weight: 100; color: red; text-align:right; padding-right:15px">@Html.Raw(TempData["DisplayMessage"])</div>
        <hr style="margin-top: 0;">
        <div class="test viewlist">
            <div class="panel panel-default">

                <div class="row detail test-gird">
                    <div class="col-xs-24 col-sm-24 col-md-24">
                        <div class="section-admin container-fluid">
                            <table class="table table-striped table-bordered table-hover table-middle">
                                <tr>
                                    <th>#</th>
                                    <th>
                                        Chủ đề
                                    </th>
                                    <th>
                                        Thời gian hoàn thành
                                    </th>
                                    <th>
                                        Điểm
                                    </th>
                                    <th>
                                        Ngày làm bài
                                    </th>
                                    <th></th>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr >
                                        <td>
                                            @i
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ExamTitle)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TakeExamTime)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Score)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CreatedDate)
                                        </td>
                                        <td>
                                            <a type="button" style="border-radius:5px" href=" @Url.Action("ExamDetail", "ExamHistory", new { id = item.ExamHisID })" class="btn  btn-info "> <i class="fa fa-inbox" style="font-size:15px" aria-hidden="true"></i> </a>
                                        </td>
                                    </tr>
                                    i++;
                                }
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>